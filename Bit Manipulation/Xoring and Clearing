class Solution{
    public:
    void printArr(int arr[],int n)
    {
        for(int i=0; i<n; i++){
           cout << arr[i] << " "; 
       }
        
        cout<<endl;
    }
    
    void setToZero(int arr[],int n)
    {
        memset(arr, 0, n*sizeof(arr[0]));
   
    }
    
    void xor1ToN(int arr[], int n)
    {
          for(int i=0; i<n; i++){
          
          arr[i]=arr[i]^i;
      }
    }
};
