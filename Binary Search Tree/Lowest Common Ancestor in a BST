Node* LCA(Node *root, int n1, int n2)
{
    // base case
    if(root==NULL)
        return NULL;
    
    if(n1<root->data and n2<root->data)
    {
        return LCA(root->left,n1,n2);
    }
    if(n1>root->data and n2 > root->data)
    {
        return LCA(root->right,n1,n2);
    }
    
    //returning the lowest common ancestor.
    return root;
}
